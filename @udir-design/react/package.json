{
  "name": "@udir-design/react",
  "repository": {
    "type": "git",
    "url": "github:Utdanningsdirektoratet/designsystem",
    "directory": "@udir-design/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./alpha": {
      "import": "./dist/alpha.js",
      "require": "./dist/alpha.cjs"
    },
    "./beta": {
      "import": "./dist/beta.js",
      "require": "./dist/beta.cjs"
    },
    "./html": {
      "types": "./dist/html.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "vite build",
    "inject": "pnpm i",
    "lint:cycles": "pnpm madge --circular ./src/alpha.ts",
    "test": "vitest --project unit",
    "typecheck": "tsc --build",
    "dev": "storybook dev --port 6005",
    "build:storybook": "storybook build --stats-json",
    "serve:storybook": "pnpm dlx http-server ./storybook-static",
    "test:storybook": "pnpm exec playwright install --with-deps chromium && pnpm vitest --project storybook"
  },
  "dependencies": {
    "@digdir/designsystemet-react": "1.5.0",
    "@navikt/aksel-icons": "^7.30.1",
    "@udir-design/css": "workspace:*",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@types/diffable-html": "^5.0.2",
    "@types/mdast": "^4.0.4",
    "@types/ramda": "^0.30.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/remark-heading-id": "^1.0.0",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axe-core": "^4.10.3",
    "axe-playwright": "^2.2.2",
    "chromatic": "^13.1.4",
    "clsx": "2.1.1",
    "diffable-html": "^6.0.1",
    "eslint-plugin-storybook": "^9.1.5",
    "hast-util-from-html": "^2.0.3",
    "hast-util-is-element": "^3.0.0",
    "hast-util-to-html": "^9.0.5",
    "highcharts": "^12.4.0",
    "highcharts-react-official": "^3.2.2",
    "markdown-to-jsx": "^7.7.13",
    "mdast-util-to-hast": "^13.2.0",
    "playwright": "^1.55.0",
    "ramda": "^0.31.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-style-stringify": "^1.2.0",
    "remark-gfm": "^4.0.1",
    "remark-heading-id": "^1.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "sass": "^1.92.1",
    "storybook": "^9.1.5",
    "storybook-addon-pseudo-states": "^9.1.5",
    "storybook-addon-tag-badges": "^2.0.2",
    "unified": "^11.0.5",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@types/react": "^18.3.7 || ^19.0.0",
    "react": "^18.3.1 || ^19.0.0",
    "react-dom": "^18.3.1 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  }
}
