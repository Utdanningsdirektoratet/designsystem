{
  "name": "@udir-design/react",
  "repository": {
    "type": "git",
    "url": "github:Utdanningsdirektoratet/designsystem",
    "directory": "@udir-design/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "main": "./dist/stable.js",
  "types": "./dist/stable.d.ts",
  "exports": {
    ".": {
      "import": "./dist/stable.js",
      "require": "./dist/stable.cjs"
    },
    "./alpha": {
      "import": "./dist/alpha.js",
      "require": "./dist/alpha.cjs"
    },
    "./beta": {
      "import": "./dist/beta.js",
      "require": "./dist/beta.cjs"
    },
    "./html": {
      "types": "./dist/html.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "vite build",
    "inject": "pnpm i",
    "lint:cycles": "pnpm madge --circular ./src/alpha.ts",
    "test:all": "vitest --outputFile.json test-reports/all.json",
    "test": "vitest --project unit --outputFile.json test-reports/unit.json",
    "typecheck": "tsc --build",
    "dev": "storybook dev --port 6005",
    "build:storybook": "storybook build --stats-json",
    "serve:storybook": "pnpm dlx http-server ./storybook-static",
    "test:storybook": "pnpm exec playwright install --with-deps chromium && vitest --project storybook --outputFile.json test-reports/storybook.json --outputFile.html test-reports/storybook/index.html",
    "view-test-report:storybook": "pnpm dlx http-server ./test-reports/storybook"
  },
  "dependencies": {
    "@digdir/designsystemet-react": "catalog:",
    "@digdir/designsystemet-types": "catalog:",
    "@udir-design/css": "workspace:*",
    "@udir-design/icons": "workspace:*",
