{
  "name": "@udir-design/symbols",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "github:Utdanningsdirektoratet/designsystem",
    "directory": "@udir-design/symbols"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./metadata": {
      "import": {
        "types": "./dist/metadata.d.ts",
        "default": "./dist/metadata.mjs"
      },
      "require": {
        "types": "./dist/metadata.d.ts",
        "default": "./dist/metadata.cjs"
      }
    },
    "./svg/*.svg": "./dist/svg/*.svg",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "generate": "svgr symbols --typescript --ext tsx --no-prettier --index-template config/index-template.js --out-dir src && prettier --write ./src",
    "update:metadata": "node config/metadata.js",
    "copy:utils": "shx mkdir -p src/util && shx cp -r util/* src/util || exit 0",
    "copy:svgs": "shx mkdir -p dist/svg && shx cp -r symbols/*.svg dist/svg || exit 0",
    "prebuild": "pnpm run copy:utils && pnpm run generate",
    "build": "tsup src/index.tsx --format esm,cjs --dts --sourcemap --clean --external react",
    "postbuild": "pnpm run update:metadata && pnpm run copy:svgs",
    "fetch-new:symbols": "tsx config/figma/index.ts",
    "generate:pngs": "node config/svg-to-png.js"
  },
  "devDependencies": {
    "@babel/types": "catalog:",
    "@figma/rest-api-spec": "catalog:",
    "@svgr/cli": "catalog:",
    "@svgr/core": "catalog:",
    "@types/react": "catalog:",
    "fast-glob": "catalog:",
    "js-yaml": "catalog:",
    "lodash-es": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "sharp": "catalog:",
    "shx": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
