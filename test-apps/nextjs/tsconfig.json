{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "paths": {
      "@udir-design/demos/*": ["../../@udir-design/react/demo-pages/*"],
      ".storybook/*": ["../../@udir-design/react/.storybook/*"],
      "src/components/*": ["./node_modules/@udir-design/react/dist/alpha"],
      "src/utilities/form/*": [
        "./node_modules/@udir-design/react/dist/utilities/form/alpha"
      ],
      "src/utilities/dataVisualization": [
        "./node_modules/@udir-design/react/dist/utilities/dataVisualization/alpha"
      ],
      "src/utilities/*": ["./node_modules/@udir-design/react/dist/alpha"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": ["@udir-design/theme", "@udir-design/react/html"],
    "target": "ES2017"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "../../test-apps/nextjs/.next/types/**/*.ts",
    "../../dist/test-apps/nextjs/.next/types/**/*.ts",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": ["node_modules", "jest.config.ts", "**/*.spec.ts", "**/*.test.ts"]
}
