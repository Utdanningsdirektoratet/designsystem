{
  "border-radius": {
    "1": {
      "$type": "dimension",
      "$value": "min({border-radius.base}*0.5,{border-radius.scale})"
    },
    "2": {
      "$type": "dimension",
      "$value": "min({border-radius.base},{border-radius.scale}*2)"
    },
    "3": {
      "$type": "dimension",
      "$value": "min({border-radius.base}*2,{border-radius.scale}*3)"
    },
    "4": {
      "$type": "dimension",
      "$value": "min({border-radius.base}*3,{border-radius.scale}*4)"
    },
    "5": {
      "$type": "dimension",
      "$value": "{border-radius.base}"
    },
    "6": {
      "$type": "dimension",
      "$value": "9999"
    },
    "base": {
      "$type": "dimension",
      "$value": "12"
    },
    "scale": {
      "$type": "dimension",
      "$value": "4"
    }
  }
}