{
  "udir": {
    "font-family": {
      "$type": "fontFamilies",
      "$value": "Montserrat"
    },
    "font-weight": {
      "medium": {
        "$type": "fontWeights",
        "$value": "Medium"
      },
      "semibold": {
        "$type": "fontWeights",
        "$value": "SemiBold"
      },
      "regular": {
        "$type": "fontWeights",
        "$value": "Regular"
      }
    },
    "font-scale": {
      "_base": {
        "$type": "number",
        "$value": "{udir.font-scale.primary._base}"
      },
      "_ratio": {
        "$type": "number",
        "$value": "{udir.font-scale.primary._ratio}"
      }
    }
  }
}