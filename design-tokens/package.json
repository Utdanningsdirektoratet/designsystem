{
  "name": "@udir-design/tokens",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./dist/udir.css",
    "./*": "./dist/*"
  },
  "scripts": {
    "create": "designsystemet tokens create",
    "build": "pnpm clean && designsystemet tokens build -t ./src -o ./dist",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@digdir/designsystemet": "catalog:",
    "rimraf": "catalog:"
  },
  "devEngines": {
    "packageManager": {
      "name": "pnpm",
      "onFail": "error"
    }
  }
}
