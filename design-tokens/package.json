{
  "name": "@udir-design/tokens",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./dist/udir.css",
    "./*": "./dist/*"
  },
  "scripts": {
    "create": "designsystemet tokens create --main-colors  'accent:#303030' --neutral-color '#dedede' --support-colors 'brand1:#76c69d' 'brand2:#bed5e8' 'brand3:#e5ceae' --theme 'udir' --out-dir './src'",
    "build": "pnpm clean && designsystemet tokens build -t ./src -o ./dist",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@digdir/designsystemet": "1.0.0-next.44",
    "rimraf": "^6.0.1"
  }
}
